{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": false
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": 0,
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        // "quotes": ["error", "double"],
        "semi": [2, "always"]
     }
}
